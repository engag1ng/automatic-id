{
  "name": "my-flask-desktop-app",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.example.flaskapp",
    "productName": "FlaskDesktopApp",
    "files": [
      "**/*",
      "../backend/**/*",
      "../templates/**/*",
      "../static/**/*",
      "../app.py",
      "../start.py",
      "../log.csv"
    ],
    "extraResources": [
      {
        "from": "../.venv",
        "to": ".venv"
      }
    ]
  },
  "devDependencies": {
    "electron": "^36.5.0",
    "electron-builder": "^26.0.12"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": ""
}
